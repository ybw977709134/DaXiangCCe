<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="preference">
        <attr name="title" format="string|reference" />
        <attr name="content" format="string|reference" />
        <attr name="titleColor" format="color|reference" />
        <attr name="contentColor" format="color|reference" />
        <attr name="topLineColor" format="color|reference" />
        <attr name="bottomLineColor" format="color|reference" />
        <attr name="topLineHeight" format="dimension|reference" />
        <attr name="bottomLineHeight" format="dimension|reference" />
        <attr name="bottomMarginLeft" format="dimension|reference" />
        <attr name="drawable" format="reference" />
        <attr name="icon" format="reference" />
        <attr name="leftIcon" format="reference" />
        <!-- <attr name="enabled" format="boolean" /> -->
        <attr name="hasMore" format="boolean" />
        <!-- icon of more -->
        <attr name="hasCheck" format="boolean" />
        <!-- show checkbox -->
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

</resources>